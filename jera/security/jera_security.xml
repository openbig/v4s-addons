<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="group_jera" model="res.groups" context="{'noadmin':True}">
            <field name="name">JERA</field>
        </record>

        <record id="group_jera_reg_users" model="res.groups" context="{'noadmin':True}">
            <field name="name">JERA register users</field>
        </record>

	    <record model="res.users" id="jera_user">
		    <field name="name">JERA Service</field>
		    <field name="login">jera_service</field>
		    <field name="password">jera_service</field>
		    <field eval="[(6,0,[ref('group_jera_reg_users')])]" name="groups_id"/>
	    </record>

	    <record model="res.users" id="jera_anonymous_user">
		    <field name="name">JERA Anonymous</field>
		    <field name="login">jera_anonymous</field>
		    <field name="password">jera_anonymous</field>
		    <field name="address_id" ref="jera_anonym_partner_address"/>
		    <field eval="[(6,0,[ref('group_jera')])]" name="groups_id"/>
	    </record>

    </data>
</openerp>

