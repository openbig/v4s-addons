<?xml version="1.0"?>
<openerp>
<data noupdate="1">

    <record id="jera_reg_confirm_id" model="ir.actions.report.xml">
        <field name="name">HTML Confirm Registration</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">jera.users</field>
        <field name="report_name">html_confirm_register</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">genshi-raw</field>
        <field name="out_format" search="[('code', '=', 'genshi-raw')]"/>
        <field name="parser_loc">jera_register/report/confirm_register_parser.py</field>
        <field name="report_rml">jera_register/report/confirm_register.html</field>
        <field name="parser_state">loc</field>
        <field name="tml_source">file</field>
        <field name="charset">utf_8</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['jera.users']" name="models"/>
        <field name="name">html_confirm_register</field>
        <field eval="'ir.actions.report.xml,'+str(jera_reg_confirm_id)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>

</data>
</openerp>

